{
  "name": "nuxt-typescript",
  "version": "1.0.0",
  "description": "My ultimate Nuxt.ts project",
  "author": "Him-2C",
  "private": true,
  "scripts": {
    "dev:spa": "nuxt-ts",
    "start:spa": "nuxt-ts start",
    "dev": "cross-env NODE_ENV=development nodemon server/index.ts",
    "start": "cross-env NODE_ENV=production ts-node --project server/tsconfig.json server/index.ts",
    "build": "nuxt-ts build",
    "generate": "nuxt-ts generate",
    "lint": "eslint --ext .ts,.js,.vue ."
  },
  "dependencies": {
    "@nuxt/typescript-runtime": "^0.1.2",
    "@nuxtjs/axios": "^5.6.0",
    "@nuxtjs/pwa": "^3.0.0-0",
    "cross-env": "^5.2.0",
    "express": "^4.17.1",
    "nuxt": "^2.9.1",
    "nuxt-property-decorator": "^2.3.0",
    "vue-property-decorator": "^8.2.1"
  },
  "devDependencies": {
    "@nuxt/typescript-build": "^0.1.10",
    "@nuxtjs/eslint-config": "^1.1.2",
    "@nuxtjs/eslint-config-typescript": "^0.1.2",
    "@nuxtjs/eslint-module": "^1.0.0",
    "@nuxtjs/vuetify": "^1.3.2",
    "@types/express": "^4.17.1",
    "@typescript-eslint/parser": "^2.0.0",
    "babel-eslint": "^10.0.2",
    "eslint": "^6.2.1",
    "eslint-plugin-nuxt": ">=0.4.3",
    "nodemon": "^1.19.1"
  }
}
